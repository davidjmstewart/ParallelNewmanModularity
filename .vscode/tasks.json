{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Library Build",
      "type": "shell",
      "command": "gcc",
      "args": [
        "-o",
        "CDUtils.o",
        "-c",
        "CDUtils.c",
        "-g",
        "-fopenmp",
        "-Wall",
        "-Werror",
        "-Wpedantic",
        "-Waggressive-loop-optimizations",
        // "-O3",
        // "-march=native"
      ],
      "options": {
        "cwd": "${workspaceFolder}/lib"
      }
    },
    {
      "label": "Application Build",
      "type": "shell",
      "command": "gcc",
      "args": [
        "-fopenmp",
        "-g",
        "main.c",
        "-o",
        "main",
        "./lib/CDUtils.o",
        "-lm",
        "-Wall",
        "-Werror",
        "-Wpedantic",
        "-Waggressive-loop-optimizations",
          // "-O3",
          // "-march=native"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    },
        {
          "label": "BuildDotProductBenchmark",
          "type": "shell",
          "command": "gcc",
          "args": [
            "-fopenmp",
            "-g",
            "./benchmarking/dot_product/dot_product_benchmarking.c",
            "-o",
            "./benchmarking/dot_product/dot",
            "./lib/CDUtils.o",
            "-lm",
            "-Wall",
            "-Wpedantic",
            "-Waggressive-loop-optimizations",
          // "-O3",
          // "-march=native"
          ],
          "options": {
            "cwd": "${workspaceFolder}"
          }
        },
        {
          "label": "BuildMvmBenchmark",
          "type": "shell",
          "command": "gcc",
          "args": [
            "-fopenmp",
            "-g",
            "./benchmarking/matrix_vector_multiplication/mvm_benchmarking.c",
            "-o",
            "./benchmarking/matrix_vector_multiplication/mvm",
            "./lib/CDUtils.o",
            "-lm",
            "-Wall",
            "-Wpedantic",
            "-Waggressive-loop-optimizations",
            "-O3",
            "-march=native",
            "-funroll-loops",
            
            // "-fopt-info-vec-all",
          ],
          "options": {
            "cwd": "${workspaceFolder}"
          }
        },
        {
          "label": "BuildSgmmBenchmark",
          "type": "shell",
          "command": "gcc",
          "args": [
            "-fopenmp",
            "./lib/CDUtils.o",
            "-g",
            "./benchmarking/subgraph_modularity_matrix/sgmm_benchmarking.c",
            "-o",
            "./benchmarking/subgraph_modularity_matrix/sgmm",
            "-lm",
            "-Wall",
            "-Wpedantic",
            "-Waggressive-loop-optimizations",
            // "-O3",
            // "-march=native",
            // "-funroll-loops",
            // "-fopt-info-vec-all",
          ],
          "options": {
            "cwd": "${workspaceFolder}"
          }
        },
        {
          "label": "BuildMembershipVectorBenchmark",
          "type": "shell",
          "command": "gcc",
          "args": [
            "-fopenmp",
            "./lib/CDUtils.o",
            "-g",
            "./benchmarking/membership_vector/membership_vector_benchmarking.c",
            "-o",
            "./benchmarking/membership_vector/membership_vector",
            "-lm",
            "-Wall",
            "-Wpedantic",
            "-Waggressive-loop-optimizations",
            "-O3",
            "-march=native",
            // "-funroll-loops",
            // "-fopt-info-vec-all",
          ],
          "options": {
            "cwd": "${workspaceFolder}"
          }
        },
                {
                  "label": "BuildMatrixVectorTaskingBencmark",
                  "type": "shell",
                  "command": "gcc",
                  "args": [
                    "-fopenmp",
                    "./lib/CDUtils.o",
                    "-g",
                    "./benchmarking/tasking/matrixTasking.c",
                    "-o",
                    "./benchmarking/tasking/matrixTasking",
                    "-lm",
                    "-Wall",
                    "-Wpedantic",
                    "-Waggressive-loop-optimizations",
                    // "-O3",
                    // "-march=native",
                    // "-funroll-loops",
                    // "-fopt-info-vec-all",
                  ],
                  "options": {
                    "cwd": "${workspaceFolder}"
                  }
                },
{
      "label": "Build",
      "dependsOrder": "sequence",
      "dependsOn": [
        "Library Build",
        "Application Build"
      ]
    }
  ]
}